<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffcccccc" android:endColor="#ffcccbcb" android:angle="270.0" android:centerY="0.5" android:centerColor="#ffcccccc" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffff6000" android:endColor="#ffff6000" android:angle="270.0" android:centerY="0.5" android:centerColor="#ffff6000" />
            </shape>
        </clip>
    </item>
</layer-list>