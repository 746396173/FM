<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.5dip" />
            <gradient android:startColor="#ff242b39" android:endColor="#ff242b39" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff242b39" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.5dip" />
                <gradient android:startColor="@color/playerprogress_1_new" android:endColor="@color/playerprogress_1_new" android:angle="270.0" android:centerY="0.75" android:centerColor="@color/playerprogress_1_new" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.5dip" />
                <gradient android:startColor="#00242b39" android:endColor="#00242b39" android:angle="270.0" android:centerY="0.75" android:centerColor="#00242b39" />
            </shape>
        </clip>
    </item>
</layer-list>